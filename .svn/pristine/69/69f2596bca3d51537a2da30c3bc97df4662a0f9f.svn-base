if ( ! function_exists('getAllDataLayerPriority'))
{
    
    function getAllDataLayerPriority()
    {
        $CI =& get_instance();
        $CI->load->model('m_auth_login');	
        $layer_priority = $CI->m_auth_login->getAllDataLayerPriority();
        
        return $layer_priority;
    }

}

